<?xml version='1.0' encoding='utf-8'?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
  <key id="d3" for="edge" attr.name="type" attr.type="string" />
  <key id="d2" for="edge" attr.name="label" attr.type="string" />
  <key id="d1" for="node" attr.name="type" attr.type="string" />
  <key id="d0" for="node" attr.name="label" attr.type="string" />
  <graph edgedefault="directed">
    <node id="cat">
      <data key="d0">cat</data>
      <data key="d1">entity</data>
    </node>
    <node id="mouse">
      <data key="d0">mouse</data>
      <data key="d1">entity</data>
    </node>
    <node id="dog">
      <data key="d0">dog</data>
      <data key="d1">entity</data>
    </node>
    <node id="pursuit">
      <data key="d0">pursuit</data>
      <data key="d1">entity</data>
    </node>
    <edge source="cat" target="mouse">
      <data key="d2">chased</data>
      <data key="d3">relationship</data>
    </edge>
    <edge source="mouse" target="pursuit">
      <data key="d2">escaped</data>
      <data key="d3">relationship</data>
    </edge>
  </graph>
</graphml>
